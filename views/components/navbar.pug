nav.navbar.fixed-top.navbar-toggleable-md.navbar-expand-lg.navbar-light.bg-light
    button.navbar-toggler.navbar-toggler-right(type='button', data-toggle='collapse', data-target='#navbarNavAltMarkup', aria-controls='navbarNavAltMarkup', aria-expanded='false', aria-label='Toggle navigation')
        span.navbar-toggler-icon
    a.navbar-brand(href='/')
        //img(src="../images/monogram.png").img-logo-sm
        | #{title}
    #navbarNavAltMarkup.collapse.navbar-collapse
        .navbar-nav
            a.nav-item.nav-link.active(href='/')
                | Home
                span.sr-only (current)
            if isAuthenticated
                a.nav-item.nav-link#signout(href='/auth/sign-out') Sign Out
                a.nav-item.nav-link(href='/auth/profile') Profile
            else
                a.nav-item.nav-link(href='/auth/sign-in') Sign In
            a.nav-item.nav-link(href='/tic-tac-toe') tic-tac-toe
            a.nav-item.nav-link(href='/pizza-shop') pizza-shop
            a.nav-item.nav-link(href='/about') About
            a.nav-item.nav-link(href='/chat-room') Chat
            script.
                $('#signout').click(function()  {
                    var auth2 = gapi.auth2.getAuthInstance();
                    auth2.signOut().then(function () {
                        console.log('User signed out from google.');
                    });
                })


